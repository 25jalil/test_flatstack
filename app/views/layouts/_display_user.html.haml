.user_position
  - if signed_in?
    = link_to current_user.email, user_path(current_user), id: "position1"
    = link_to "Sign out", signout_path, method: "delete", id: "position2"
  - else
    = link_to "Sign in", signin_path, id: "position1"
    = link_to "Sign up", new_user_path, id: "position2"

